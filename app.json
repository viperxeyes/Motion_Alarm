{
  "expo": {
    "name": "MotionSensor",
    "slug": "MotionSensor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Images/logo.png",
    "splash": {
      "image": "./assets/Images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#242736"
    },
    "updates": {
